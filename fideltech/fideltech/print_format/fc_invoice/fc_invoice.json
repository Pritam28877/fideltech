 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"widtd=device-widtd, initial-scale=1.0\">\n    <title>Invoice</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            line-height: 1.6;\n            margin: 0;\n            padding: 0;\n            background-color: #f4f4f4;\n            width: 100%;\n        }\n   \n    </style>\n</head>\n<body>\n    <div style=\"width: 100%; text-align: center;\">\n<img style=\"height: 80px; width: 300px;\" src=\"{{ frappe.utils.get_url('/files/FC_logo.png') }}\" alt=\"Logo\">\n</div>\n<hr style=\" margin-top: 5px;\n            border: none;           \n            height: 2px;              \n            background-color: #333;  \n            width: 100%;\">\n    <div style=\"   width: 100%; widtd: 30%; text-align: center; font-size: 10pt; color: black; margin-top: -14px;\"><b>Invoice Number:-{{doc.name}} </b></div>\n        <!--<p style=\"font-size: 8pt;\"> </p>-->\n      <table>\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Customer Name:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>{{doc.customer}}</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Date:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{doc.posting_date}}</td>\n    </tr>\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Payment Due Date:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{doc.due_date}}</td>\n    </tr>\n    {% if doc.custom_po_number %}\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>PO:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{ doc.custom_po_number }}</td>\n    </tr>\n    {% endif %}\n    {% if doc.custom_grn_number %}\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>GRN Number</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{ doc.custom_grn_number }}</td>\n    </tr>\n    {% endif %}\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Address:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px;\">{{doc.address_display}}</td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px;\"></td>\n    </tr>\n     {% if doc.custom_tax_no %}\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Tax No:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{doc.custom_tax_no}}</td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n    </tr>\n    {% endif %}\n    <tr>\n        {% if doc.tax_id  %}\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Tax ID:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{doc.tax_id}}</td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n    </tr>\n    {% endif %}\n    {% if doc.custom_attn %}\n    <tr>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"><b>Atten:</b></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\">{{doc.custom_attn}}</td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n        <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black; padding-bottom: 3px; line-height: 1pt;\"></td>\n    </tr>\n    {% endif %}\n</table>\n        <div style=\"margin-left: 10pt;\">\n        <strong><p style=\"fornt-size: 8pt; \">Description</p></strong>\n        <div class=\"contact-info\">\n           \n            <p style=\"font-size: 8pt; line-height: 8pt;\">consultant Name: {{doc.custom_employname}}</p>\n             \n             {% if doc.custom_consultrator_id %}\n            <p style=\"font-size: 8pt; line-height: 8pt;\">consultant ID: {{doc.custom_consultrator_id}}</p>\n             {% endif %}\n            {% if doc.posting_date %}\n            <p style=\"font-size: 8pt; line-height: 8pt;\">Service period: {{ frappe.utils.formatdate(doc.posting_date, \"MMMM\") }} {{ frappe.utils.formatdate(doc.posting_date, \"yyyy\") }}</p>\n             {% endif %}\n            {% if doc.custom_project_name %}\n            <p style=\"font-size: 8pt; line-height: 8pt;\">Project - {{doc.custom_project_name}}</p>\n            {% endif %}\n            {% if doc.custom_designation %}\n            <p style=\"font-size: 8pt; line-height: 8pt;\">Designation - {{doc.custom_designation}}</p>\n             {% endif %}\n        </div>\n    <table style=\"width: 100%;\">\n       <thead style=\"width: 100%;\">\n    <tr style=\"width: 100%;\">\n      <th rowspan=\"6\" style=\"border: 1px solid black; width: 10%; font-size: 10pt; color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>S/No.</b></th>\n      <th style=\"border: 1px solid black; width: 25%; font-size: 10pt; color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>item</b></th>\n      <th style=\"border: 1px solid black; width: 10%; font-size: 10pt; color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>Description</b></th>\n   \n      <th style=\"border: 1px solid black; width: 15%; font-size: 10pt;color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>Quantity</b></th>\n        <th style=\"border: 1px solid black; width: 15%; font-size: 10pt;color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>Rate</b></th>\n      <th style=\"border: 1px solid black; width: 25%; font-size: 10pt;color: black; padding-top: 7px !important; padding-bottom: 7px !important;\"><b>Amount</b></th>\n    </tr>\n  </thead>\n  <tbody style=\"width: 100%;\">\n       {% for item in doc.items %}\n       <tr style=\"width: 100%;\">\n      <td  style=\"border: 1px solid black; width: 5%; font-size: 8pt; color: black; padding-top: 5px !important; padding-bottom: 5px !important;\">{{loop.index}}.</td>\n      <td style=\"border: 1px solid black; width: 25%; font-size: 8pt; color: black; padding-top: 5px !important; padding-bottom: 5px !important;\">{{item.item_name}} </td>\n      <td style=\"border: 1px solid black; width: 17%; font-size: 8pt; color: black; padding-top: 5px !important; padding-bottom: 5px !important;\">{{item.description}} </td>\n   \n      <td style=\"border: 1px solid black; width: 7%; font-size: 8pt;color: black; padding-top: 5px !important; padding-bottom: 5px !important; text-align: end;\">{{item.qty}} {{item.custom_type}} </td>\n        <td style=\"border: 1px solid black; width: 10%; font-size: 8pt;color: black; padding-top: 5px !important; padding-bottom: 5px !important; text-align: end;\">\u00a5  {{ \"{:,.2f}\".format(item.rate|float) }}</td>\n      <td style=\"border: 1px solid black; width: 20%; font-size: 8pt;color: black; padding-top: 5px !important; padding-bottom: 5px !important; text-align: end;\">   \u00a5 {{ \"{:,.2f}\".format(item.custom_amount1|float) }}</td>\n    </tr>\n     {% endfor %}\n  </tbody>\n    </table>\n     </table>\n    <table style=\"width: 100%; margin-top: 0pt;\">\n        <tbody style=\"width: 100%;\">\n            <tr style=\"width: 100%;\">\n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black;  text-align: start; margin-left: 40pt;\"></td>\n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt; color: black;  text-align: end;\"></td>\n       <td style=\"border: 1px solid white; width: 25%; font-size: 8pt;color: black; text-align: start; font-weight: 600; line-height: 1pt;\">Total</td>\n       <td style=\"border: 1px solid white; width: 25%; font-size: 10pt;color: black; text-align: end; line-height: 1pt;\"><p >\u00a5 {{ \"{:,.2f}\".format(doc.custom_total1) }}</p></td>\n    </tr>\n           <tr style=\"width: 100%;\">\n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black;\"></td>\n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt; color: black;\"></td>\n   \n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt;color: black; text-align: start; line-height: 1pt; font-weight: 600; \">Consumption Tax 10%:</td>\n    \n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt;color: black; text-align: end; line-height: 1pt; \">\u00a5 {{ \"{:,.2f}\".format(doc.custom_tax_amount|float) }}</td>\n    </tr>\n     <tr style=\"width: 100%;\">\n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black;\"></td>\n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt; color: black;\"></td>\n   \n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt;color: black; text-align: start; line-height: 1pt; font-weight: 600; \">Grand Total:\n    \n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt;color: black; text-align: end; line-height: 1pt; \">\u00a5 {{ \"{:,.2f}\".format(doc.custom_grand_total_1|float) }}</td>\n    </tr>\n     <tr style=\"width: 100%;\">\n      <td style=\"border: 1px solid white; width: 25%; font-size: 8pt; color: black;\"></td>\n      <td style=\"border: 1px solid white; width: 25%; font-size: 10pt; color: black;\"></td>\n     <td colspan=\"2\" style=\"border: 1px solid white; width: 25%; font-size: 8pt;color: black; text-align: start; padding-top: 1px !important;\"><b>